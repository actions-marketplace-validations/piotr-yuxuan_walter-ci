<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>github</id>
            <username>${env.WALTER_GITHUB_USERNAME}</username>
            <password>${env.WALTER_GITHUB_PASSWORD}</password>
        </server>
        <server>
            <id>clojars</id>
            <username>${env.WALTER_CLOJARS_USERNAME}</username>
            <password>${env.WALTER_CLOJARS_PASSWORD}</password>
        </server>
    </servers>
</settings>
